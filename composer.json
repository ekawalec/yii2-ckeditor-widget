{
    "name": "s4studio/yii2-ckeditor-widget",
    "description": "CKEditor widget for Yii2.",
    "type": "yii2-extension",
    "keywords": [
        "s4studio",
        "yii",
        "yii2",
        "yii 2",
        "extension",
        "widget",
        "ckeditor"
    ],
    "homepage": "http://yiiwheels.com/extension/ckeditor-widget",
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "s4studio",
            "email": "biuro@s4studio.pl",
            "homepage": "http://s4studio.pl",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/s4studio/yii2-ckeditor-widget/issues",
        "source": "https://github.com/s4studio/yii2-ckeditor-widget"
    },
    "require": {
        "yiisoft/yii2": "^2.0",
        "ckeditor/ckeditor": "dev-full/4.11.1 as 4.11"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "dosamigos\\ckeditor\\": "src"
        }
    },
    "config": {
        "fxp-asset": {
            "installer-paths": {
                "npm-asset-library": "vendor/npm"
            }
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1-dev"
        }
    }
}
